{
  "name": "minimal-theme",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "sass": "sass scss/main.scss:css/style.css --style compressed",
    "watch": "sass --watch scss/main.scss:css/style.css --style compressed",
    "prefix": "postcss css/style.css --use autoprefixer -o css/style.css",
    "build": "npm-run-all sass prefix",
    "start": "npm run watch"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "sass": "^1.85.1"
  }
}
